score_str = input("Enter a score between 0.0 and 1.0: ")
try:
    score = float(score_str)
except ValueError:
    print("Error: Please enter a numeric input.")
    quit()
if score < 0.0 or score > 1.0:
    print("Error: Score out of range.")
    quit()
if score >= 0.9:
    print("A")
elif score >= 0.8:
    print("B")
elif score >= 0.7:
    print("C")
elif score >= 0.6:
    print("D")
else:
    print("F")
